<%= link_to "<< Back to List", pages_path %>

<div>
	<h2>Page Details</h2>

	<table>
		<tr>
			<th>Name</th>
			<td><%= @page.name %></td>
		</tr>

		<tr>
			<th>Subject id</th>
			<td><%= @page.subject_id %></td>
		</tr>

		<tr>
			<th>Permalink</th>
			<td><%= @page.permalink %></td>
		</tr>

		<tr>
			<th>Position</th>
			<td><%= @page.position %></td>
		</tr>

		<tr>
			<th>Visible</th>
			<td><%= @page.visible %></td>
		</tr>

		<tr>
			<th>Created</th>
			<td><%= @page.created_at %></td>
		</tr>

		<tr>
			<th>Updated</th>
			<td><%= @page.updated_at %></td>
		</tr>

	</table>
